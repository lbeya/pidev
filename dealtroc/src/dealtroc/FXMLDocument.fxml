      <TextField fx:id="texte" layoutX="126.0" layoutY="256.0" />
      <Button layoutX="16.0" layoutY="256.0" mnemonicParsing="false" onAction="#Ajouter_commentaire" text="commenter" />
      <Button layoutX="283.0" layoutY="231.0" mnemonicParsing="false" onAction="#Supprimer_commentaire" prefHeight="1.0" prefWidth="77.0" text="supprimer" />
