<AnchorPane id="AnchorPane" prefHeight="375.0" prefWidth="506.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dealtroc.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="22.0" layoutY="14.0" onAction="#handleButtonAction" text="Click Me!" />
